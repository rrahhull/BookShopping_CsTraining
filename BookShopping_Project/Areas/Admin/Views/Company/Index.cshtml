@model BookShopping_Project.Models.Company
<div class="row">
    <div class="col-9">
        <h2 class="text-primary">Company List</h2>
    </div>
    <div class="col-3">
        <a asp-action="Upsert" class="btn btn-primary">
            <i class="fas fa-plus"></i>&nbsp; Create New Company
        </a>
    </div>
</div>
<div class="border p-2 m-2">
    <table id="tblData" class="table table-bordered table-striped">
        <thead>
            <tr>
                <th>Name</th>
                <th>City</th>
                <th>State</th>
                <th>Postal Code</th>
                <th>Phone No.</th>
                <th>Is Authorized Company</th>
                <th>Actions</th>
            </tr>
        </thead>
    </table>
</div>
@section scripts
{
<script src="~/js/Company.js"></script>
}