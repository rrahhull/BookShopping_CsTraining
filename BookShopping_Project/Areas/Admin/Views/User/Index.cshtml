<div class="row">
    <div class="col-9">
        <h2 class="text-primary">User List</h2>
    </div>
    <div class="col-3">
        <a asp-area="Identity" class="btn btn-info" asp-page="/Account/Register">
            <i class="fas fa-plus"></i>
            Create New User
        </a>
    </div>
</div>
<div class="border">
    <table id="tbldata" class="table table-bordered table-striped">
        <thead>
            <tr class="table-info">
                <th>Name</th>
                <th>Email</th>
                <th>Phone Number</th>
                <th>Company</th>
                <th>Role</th>
                <th></th>
            </tr>
        </thead>
    </table>
</div>
@section scripts
{
<script src="~/js/User.js"></script>
}